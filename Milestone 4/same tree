class Solution {
public:
    bool isSameTree(TreeNode* p, TreeNode* q) {
        if(p==NULL && q==NULL)
            return true;
        if(p==NULL && q!=NULL)
            return false;
        if(p!=NULL && q==NULL)
            return false;
         bool left = isSameTree(p->left , q->left);
         bool val=p->val==q->val;
        bool right= isSameTree(p->right, q->right);
        
        if(left && right && val)

        return true;
    return false;
    }
};
